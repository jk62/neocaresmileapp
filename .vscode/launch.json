  "version": "0.2.0",
  "configurations": [
    {
      "name": "neocaresmileapp (debug mode, dev flavor)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev"]
    },
    {
      "name": "neocaresmileapp (debug mode, prod flavor)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "args": ["--flavor", "prod"]
    },
    {
      "name": "neocaresmileapp (profile mode, dev flavor)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev"]
    },
    {
      "name": "neocaresmileapp (profile mode, prod flavor)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_prod.dart",
      "args": ["--flavor", "prod"]
    },
    {
      "name": "neocaresmileapp (release mode, dev flavor)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev"]
    },
    {
      "name": "neocaresmileapp (release mode, prod flavor)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_prod.dart",
      "args": ["--flavor", "prod"]
    }
  ]
}

